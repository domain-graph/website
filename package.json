{
  "name": "domain-graph-website",
  "version": "0.0.1",
  "description": "Blank web application built with Typescript and LESS",
  "main": "index.js",
  "scripts": {
    "data": "ts-node ./src/tools/convert.ts",
    "prebuild": "run-s lint clean build-info",
    "build": "run-s build:*",
    "build:webpack": "webpack --config webpack.prod.js",
    "build:tsc": "tsc -p tsconfig.build.json",
    "clean": "rimraf build",
    "build-info": "ts-node tools/build-info.ts > src/build-info.ts",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint src/**/*.ts",
    "prenetlify": "run-s build",
    "netlify": "netlify dev",
    "postbuild": "copyfiles -u 1 favicons/**.* build/dist",
    "pretest": "rm -rf coverage/*",
    "test": "jest",
    "prestart": "run-s build-info",
    "start": "webpack serve --config webpack.dev.js"
  },
  "keywords": [],
  "author": "",
  "private": true,
  "license": "MIT",
  "devDependencies": {
    "@netlify/functions": "^0.10.0",
    "@types/d3": "^6.2.0",
    "@types/d3-force": "^2.1.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "copyfiles": "^2.4.1",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^6.5.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^26.6.3",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "mini-css-extract-plugin": "^2.4.5",
    "netlify-cli": "^8.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-loader": "^9.2.6",
    "ts-node": "^9.0.0",
    "typescript": "^4.5.2",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "d3": "^6.2.0",
    "d3-force": "^2.1.1",
    "domain-graph": "^0.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^6.0.2",
    "use-http": "^1.0.26"
  }
}
